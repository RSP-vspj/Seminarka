{% extends 'PetkoparaCrudGeneratorBundle::base.html.twig' %}

{% block title %}
{{ parent() }} - uzivatel
{% endblock %}

{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>View uzivatel <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ uzivatel.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Uzivatelaktivni</strong></p>
        <p>
            {% if uzivatel.uzivatelAktivni %}Yes{% else %}No{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Jmeno</strong></p>
        <p>
            {{ uzivatel.jmeno }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Prijmeni</strong></p>
        <p>
            {{ uzivatel.prijmeni }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Login</strong></p>
        <p>
            {{ uzivatel.login }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Heslo</strong></p>
        <p>
            {{ uzivatel.heslo }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Odbornost</strong></p>
        <p>
            {{ uzivatel.odbornost }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Role</strong></p>
        <p>
            {{ uzivatel.role }}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('uzivatel') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('uzivatel_edit', { 'id':  uzivatel.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('uzivatel_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('uzivatel_delete', { 'id': uzivatel.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>


{% endblock %}

