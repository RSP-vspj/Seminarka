{% extends 'base.html.twig' %}

{% block title %}
    {{ parent() }} - clanek
{% endblock %}

{% block body %}

    <div class="container">
        <div id="top" class="row">
            <div class="page-header">
                <h2>View clanek <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span></h2>
            </div>
        </div>

        <div class="row">
            <div class="col-md-6">
                <p><strong>Id</strong></p>
                <p>
                    {{ clanek.id }}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Aktivniclanek</strong></p>
                <p>
                    {% if clanek.aktivniClanek %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Clanekzobrazenredakci</strong></p>
                <p>
                    {% if clanek.clanekZobrazenRedakci %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Cekanastanovenirecenzentu</strong></p>
                <p>
                    {% if clanek.cekaNaStanoveniRecenzentu %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Jsoustanovenirecenzenti</strong></p>
                <p>
                    {% if clanek.jsouStanoveniRecenzenti %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Probiharecenznirizeni</strong></p>
                <p>
                    {% if clanek.probihaRecenzniRizeni %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Recenznirizenibylodokonceno</strong></p>
                <p>
                    {% if clanek.recenzniRizeniByloDokonceno %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Recenznirizenibylozruseno</strong></p>
                <p>
                    {% if clanek.recenzniRizeniByloZruseno %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Novepodanarevize</strong></p>
                <p>
                    {% if clanek.novePodanaRevize %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Clanekprijat</strong></p>
                <p>
                    {% if clanek.clanekPrijat %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Clanekprijatsvyhradami</strong></p>
                <p>
                    {% if clanek.clanekPrijatSVyhradami %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Clanekprijatvyzadujeformalnidoplneni</strong></p>
                <p>
                    {% if clanek.clanekPrijatVyzadujeFormalniDoplneni %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Clanekzamitnutneobstalvrecenznimrizeni</strong></p>
                <p>
                    {% if clanek.clanekZamitnutNeobstalVRecenznimRizeni %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Clanekzamitnutnesplnujesablonu</strong></p>
                <p>
                    {% if clanek.clanekZamitnutNesplnujeSablonu %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Clanekzamitnuttematickynevhodny</strong></p>
                <p>
                    {% if clanek.clanekZamitnutTematickyNevhodny %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Cekasenaautorovurevizi</strong></p>
                <p>
                    {% if clanek.cekaSeNaAutorovuRevizi %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Nutnostpredatnovourevizizpetrecenzentum</strong></p>
                <p>
                    {% if clanek.nutnostPredatNovouReviziZpetRecenzentum %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Pozadaveknanahrazenirevizeautorem</strong></p>
                <p>
                    {% if clanek.pozadavekNaNahrazeniRevizeAutorem %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Lzepodatnovourevizi</strong></p>
                <p>
                    {% if clanek.lzePodatNovouRevizi %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Nejednoznacnostrecenznihorizeni</strong></p>
                <p>
                    {% if clanek.nejednoznacnostRecenznihoRizeni %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Clanekvydan</strong></p>
                <p>
                    {% if clanek.clanekVydan %}Yes{% else %}No{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Cislocasopisu</strong></p>
                <p>
                    {{ clanek.cisloCasopisu }}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Neregistrovanispoluautori</strong></p>
                <p>
                    {{ clanek.neregistrovaniSpoluautori }}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Tematickezamerenislovne</strong></p>
                <p>
                    {{ clanek.tematickeZamereniSlovne }}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Tematickezamereni</strong></p>
                <p>
                    {{ clanek.tematickeZamereni }}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Nabidnoutznovuodmitnutyclanek</strong></p>
                <p>
                    {% if clanek.nabidnoutZnovuOdmitnutyClanek %}{{ clanek.nabidnoutZnovuOdmitnutyClanek|date('Y-m-d') }}{% endif %}
                </p>

            </div>
            <div class="col-md-6">
                <p><strong>Uzaverkacislacasopisu</strong></p>
                <p>
                    {% if clanek.uzaverkaCislaCasopisu %}{{ clanek.uzaverkaCislaCasopisu|date('Y-m-d') }}{% endif %}
                </p>

            </div>
        </div>

        <hr/>


        <div class="form-group">
            <a class="btn btn-default" href="{{ path('clanek') }}">
                <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
                Back to list
            </a>
            <a class="btn btn-primary" href="{{ path('clanek_edit', { 'id':  clanek.id }) }}">
                <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                Edit
            </a>
            <a class="btn btn-info" href="{{ path('clanek_new') }}">
                <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
                New
            </a>
            <form action="{{ path('clanek_delete', { 'id': clanek.id }) }}" method="post" style="display: inline-block">
                <input type="hidden" name="_method" value="DELETE"/>
                {{ form_widget(delete_form) }}
                <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
                    <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
                    Delete
                </button>
            </form>
        </div>
    </div>
{% endblock %}

